{"format_version":"1.19.0","minecraft:client_entity":{"description":{"identifier":"spark:starfighter","materials":{"default":"entity_alphatest"},"textures":{"default":"textures/spark/entity/vehicle/n1_starfighter"},"geometry":{"default":"geometry.spark.n1_starfighter"},"render_controllers":["controller.render.default"],"animations":{"fly":"animation.spark.starfighter.fly","main":"controller.animation.spark.starfighter.main"},"scripts":{"initialize":["variable.smoothed_turning = 0;","variable.smoothed_pitch = 0;","variable.smoothed_cam_pitch = 0;","variable.smoothed_cam_yaw = 0;","variable.smoothed_velocity = 0;","variable.in_air = 0;","variable.switch_time = 0;"],"pre_animation":["variable.smoothed_turning = Math.lerp(v.smoothed_turning, Math.clamp(query.yaw_speed, -80, 80), 0.03);","variable.smoothed_pitch = Math.lerp(v.smoothed_pitch, q.property('spark:pitch'), q.is_on_ground ? 0.2 : 0.04);","variable.smoothed_cam_pitch = Math.lerp(v.smoothed_cam_pitch, q.is_on_ground ? 0 : q.property('spark:cam_pitch'), 0.1);","variable.smoothed_cam_yaw = Math.lerp(v.smoothed_cam_yaw, q.is_on_ground ? 0 : q.property('spark:cam_yaw'), 0.1);","variable.smoothed_velocity = Math.lerp(v.smoothed_velocity, Math.sqrt(q.ground_speed), 0.02);","variable.in_air = query.is_stunned || (!query.has_rider && !query.is_on_ground && (q.life_time - variable.switch_time) > 0.6);"],"animate":["main"]}}}}
